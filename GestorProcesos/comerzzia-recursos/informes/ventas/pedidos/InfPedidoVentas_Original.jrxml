	<queryString>
		<![CDATA[SELECT
     CABECERA.ESTADO,
     CABECERA.ID_CLIE_PEDIDO AS ID_CLIE_PEDIDO,
     CABECERA.CODEMP AS CODEMP,
     CABECERA.PERIODO AS PERIODO,
     CABECERA.CODSERIE AS CODSERIE,
     CABECERA.NUMPED AS NUMPED,
     CABECERA.FECHA AS D_PROV_PEDIDOS_CAB_TBL_FECHA,
     CABECERA.FECHA_SUMINISTRO_PREV AS FECHA_SUMINISTRO_PREV,
     CABECERA.REFERENCIA_CLIENTE AS REFERENCIA_CLIENTE,
     CABECERA.PERSONA_CONTACTO AS PERSONA_CONTACTO,
     CABECERA.BASE AS BASE,
     CABECERA.IMPUESTOS AS IMPUESTOS,
     CABECERA.TOTAL AS TOTAL,
     CABECERA.OBSERVACIONES AS OBSERVACIONES,
     CABECERA.ESTADO AS ESTADO,
     CLIENTES.CODCLI AS CODCLI,
     CLIENTES.DESCLI AS DESCLI,
     CLIENTES.NOMBRE_COMERCIAL AS NOMBRE_COMERCIAL,
     CLIENTES.DOMICILIO AS DOMICILIO,
     CLIENTES.POBLACION AS POBLACION,
     CLIENTES.PROVINCIA AS PROVINCIA,
     CLIENTES.CP AS CP,
     CLIENTES.TELEFONO1 AS TELEFONO1,
     CLIENTES.FAX AS FAX,
     CLIENTES.CIF AS CIF,
     DETALLES.CODART AS CODART,
     DETALLES.LINEA AS LINEA,
     DETALLES.DESGLOSE1 AS DESGLOSE1,
     DETALLES.DESGLOSE2 AS DESGLOSE2,
     DETALLES.DESART AS DESART,
     DETALLES.DESCRIPCION_AMPLIADA AS DESCRIPCION_AMPLIADA,
     DETALLES.UNIDAD_MEDIDA AS UNIDAD_MEDIDA,
     DETALLES.CANTIDAD_MEDIDA_SOLICITADA AS CANTIDAD_MEDIDA_SOLICITADA,
     DETALLES.CANTIDAD_SOLICITADA AS CANTIDAD_SOLICITADA, 
     DETALLES.CANTIDAD_MEDIDA_ACEPTADA AS CANTIDAD_MEDIDA_ACEPTADA,         
     DETALLES.CANTIDAD_ACEPTADA AS CANTIDAD_ACEPTADA, 
     DETALLES.CANTIDAD_MEDIDA_PREPARADA AS CANTIDAD_MEDIDA_PREPARADA,         
     DETALLES.CANTIDAD_PREPARADA AS CANTIDAD_PREPARADA,      
     DETALLES.PRECIO AS PRECIO,
     DETALLES.DESCUENTO AS DESCUENTO,
     DETALLES.IMPORTE AS IMPORTE_LINEA,
     DETALLES.CANTIDAD_ACEPTADA - DETALLES.CANTIDAD_PREPARADA AS CANTIDAD_SERVIDA,
     DETALLES.LESTADO AS LESTADO,
     DETALLES.LSUBESTADO AS LSUBESTADO,
     EMPRESA.DESEMP AS EMP_DESEMP,
     EMPRESA.DOMICILIO AS EMP_DOMICILIO,
     EMPRESA.POBLACION AS EMP_POBLACION,
     EMPRESA.PROVINCIA AS EMP_PROVINCIA,
     EMPRESA.CP AS EMP_CP,
     EMPRESA.CIF AS EMP_CIF,
     EMPRESA.TELEFONO1 AS EMP_TELEFONO1,
     EMPRESA.FAX AS EMP_FAX,
     EMPRESA.LOGOTIPO AS EMP_LOGOTIPO
FROM
     D_CLIENTES_TBL CLIENTES RIGHT OUTER JOIN D_CLIE_PEDIDOS_CAB_TBL CABECERA ON CLIENTES.CODCLI = CABECERA.CODCLI
     LEFT OUTER JOIN D_CLIE_PEDIDOS_DET_TBL DETALLES ON CABECERA.ID_CLIE_PEDIDO = DETALLES.ID_CLIE_PEDIDO
     LEFT OUTER JOIN D_EMPRESAS_TBL EMPRESA ON CABECERA.CODEMP = EMPRESA.CODEMP
WHERE
     CABECERA.ID_CLIE_PEDIDO = $P{ID_CLIE_PEDIDO}]]>
